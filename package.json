{
  "name": "askflow",
  "version": "1.0.0",
  "description": "Self-hosted, white-label FAQ platform with 20+ templates and AI-powered generation",
  "license": "MIT",
  "author": "AskFlow Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexBaum-ai/askflow.git"
  },
  "homepage": "https://github.com/AlexBaum-ai/askflow#readme",
  "bugs": {
    "url": "https://github.com/AlexBaum-ai/askflow/issues"
  },
  "keywords": ["faq", "knowledge-base", "self-hosted", "white-label", "react", "express", "chatbot", "api"],
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port=3000 --host=0.0.0.0\" \"tsx watch server/index.ts\"",
    "dev:client": "vite --port=3000 --host=0.0.0.0",
    "dev:server": "tsx watch server/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production tsx server/index.ts",
    "seed": "tsx server/seed.ts",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react": "^5.0.4",
    "better-sqlite3": "^12.4.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "multer": "^1.4.5-lts.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "vite": "^6.2.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
